{
  "printWidth": 110,
  "embeddedLanguageFormatting": "off",
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,
  "arrowParens": "always",
  "trailingComma": "es5",
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "experimentalTernaries": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "parser": "vue",
        "singleAttributePerLine": false,
        "vueIndentScriptAndStyle": true,
        "htmlWhitespaceSensitivity": "css",
        "embeddedLanguageFormatting": "auto"
      }
    },
    {
      "files": "*.(m|c)js(x)",
      "options": {
        "parser": "babel"
      }
    },
    {
      "files": "*.ts(x)",
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": "*.json",
      "options": {
        "parser": "json",
        "singleQuote": false,
        "trailingComma": "none"
      }
    }
  ]
}
